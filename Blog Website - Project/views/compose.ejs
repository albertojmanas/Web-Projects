<%-include("partials/header")-%>
<h1>Compose</h1>

<form class="item" action="/compose" method="post">
  <div class="form-group">
    <label>Title</label>
    <input class="input-group" type="text" name="postTitle">
  </div>
  <div class="form-group">
    <label>Post</label>
    <textarea class="form-control" name="postBody" rows="5"></textarea>
  </div>
  <div class="form-group">
    <button class="btn btn-primary" type="submit" name="postButton">Publish</button>
  </div>
</form>
<%-include("partials/footer")-%>
